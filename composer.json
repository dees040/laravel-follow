{
    "name": "hareku/laravel-follow",
    "description": "Laravel 5 user follow system",
    "keywords": ["laravel", "follow"],
    "license": "MIT",
    "authors": [
        {
            "name": "hareku",
            "email": "hareku908@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/database": "5.4.*",
        "illuminate/support": "5.4.*",
        "nesbot/carbon": "~1.22"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
