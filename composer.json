{
    "name": "hareku/laravel-follow",
    "description": "Laravel 5 user follow system",
    "keywords": ["laravel", "follow"],
    "license": "MIT",
    "authors": [
        {
            "name": "hareku",
            "email": "hareku908@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/database": "5.4.*|5.5.*|5.6.*",
        "illuminate/support": "5.4.*|5.5.*|5.6.*",
        "nesbot/carbon": "~1.22"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "Hareku\\LaravelFollow\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hareku\\LaravelFollow\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hareku\\LaravelFollow\\FollowServiceProvider"
            ]
        }
    }
}
